{
  "name": "todo-list-app-architecture",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Riquelme Maia Rodrigues",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --watch --ignore node_modules -r tsconfig-paths/register src/main/server.ts",
    "start": "node dist/src/main/server.js",
    "build": "tsc",
    "migration:create": "yarn typeorm migration:create ./src/database/migrations/CreateTableMigrationName",
    "migration:run": "yarn typeorm-ts-node-commonjs migration:run -d ./src/main/config/orm.config.ts",
    "test": "cross-env NODE_ENV=TEST jest --noStackTrace --runInBand",
    "test:verbose": "jest --verbose --runInBand --detectOpenHandles",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --silent --noStackTrace --runInBand --watchAll",
    "test:integration": "yarn test -c ./jest.config.integ.ts",
    "test:e2e": "yarn test -c ./jest.config.e2e.ts",
    "test:setup": "cross-env NODE_ENV=TEST yarn migration:run"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.5.7",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.13",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  }
}
